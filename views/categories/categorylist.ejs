<%- include('../partials/header') %> 

<div class="container">
    <table class="table table-striped">
        <thead>
          <tr>
            <th>category Id</th> 
            <th>category Name</th>
            <th>operations</th>
          </tr>
        </thead>
        <tbody>
            <% categories.forEach(category => { %> 
                <tr>
                    <td><%= category.catId %> </td>
                    <td><%= category.catName %> </td>
                    <td><a href="/categories/<%= category._id %>/edit">Edit</a></td>
                </tr>
             <% }); %>
        </tbody>
    </table>
</div>

<%- include('../partials/footer') %>   