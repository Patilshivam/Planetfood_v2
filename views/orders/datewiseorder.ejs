            <div style="padding: 10px;">
                <h2 style="margin: 10px auto;"><span class="glyphicon glyphicon-calendar"></span> Date Wise Order</h2>
            </div> 
            <form action="/orders/datewise" method="POST">
                <label for="start">Start Date :</label>
                <div class="form-group">
                    <input class="form-control" name="start" type="date" id="start" name="trip-start" value="2020-09-10" min="2000-01-01" max="2025-12-31"> 
                </div>
                <label for="start">End Date :</label>
                <div class="form-group">
                    <input class="form-control" name="end" type="date" id="start" name="trip-start" value="2020-09-10" min="2018-01-01" max="2025-12-31"> 
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">Search</button>
